{% extends 'base.html.twig' %}

{% block title %}Music{% endblock %}

{% block body %}
    <h1>Music</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ music.id }}</td>
            </tr>
            <tr>
                <th>Music_title</th>
                <td>{{ music.musicTitle }}</td>
            </tr>
            <tr>
                <th>Singer_name</th>
                <td>{{ music.singerName }}</td>
            </tr>
            <tr>
                <th>Cover</th>
                <td>{{ music.cover }}</td>
            </tr>
            <tr>
                <th>Audio</th>
                <td>{{ music.audio }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_music_index') }}">back to list</a>

    <a href="{{ path('app_music_edit', {'id': music.id}) }}">edit</a>

    {{ include('music/_delete_form.html.twig') }}
{% endblock %}
